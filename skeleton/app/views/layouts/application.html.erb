<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <p>
      Welcome <%= current_user.user_name %>  |  <%= button_to "logout", session_url, method: :delete %>
    </p>
    <% else %>
    <%= link_to "login", new_session_url %>
    <% end %>
<%= yield %>

</body>
</html>
