<h1>Log in form</h1>

<form action="/session_tokens" method="post">
  <%= authenticity_token_field %>
  <label for="user_name">Username: </label>
  <input type="text" name="session[user_name]" value="" id="user_name">
  <br><br>
  <label for="password">Password: </label>
  <input type="password" name="session[password]" value="" id="password">
  <br><br>
  <input type="submit" name="submit" value="Log In">

</form>
